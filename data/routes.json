{
  "routes": [
    {
      "name": "countries",
      "ingestionContract": "reference.countries.csv.v1",
      "domain": "reference",
      "entity": "countries",
      "input": {
        "path": "/app/data/input/reference/countries",
        "watchMode": "hybrid",
        "pollIntervalSeconds": 5,
        "hybridPollIntervalSeconds": 60,
        "suffixFilter": ".csv"
      },
      "output": {
        "type": "both",
        "queueDestination": "axiom.data.exchange",
        "fileDestination": "/app/data/output/reference/countries",
        "addTimestampSuffix": true
      },
      "archive": {
        "processedPath": "/app/data/archive/reference/countries/processed",
        "failedPath": "/app/data/archive/reference/countries/failed",
        "ignoredPath": "/app/data/archive/reference/countries/ignored"
      },
      "logging": {
        "logFolder": "/app/data/logs"
      }
    },
